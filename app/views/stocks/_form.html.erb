<%= form_with model: stock, local: true, id: "stock-form", url: warehouse_stocks_path(warehouse) do |f| %>
  <div class="form-group">
    <%= f.select :product_id, @products.map { |p| [p.name, p.id] } %>
  </div>
  <div class="form-group">
    <%= f.label :count %>
    <%= f.number_field :count, class: "form-control", required: true %>
  </div>
  <%= f.submit "Add Stock", class: "btn btn-success", id: "submit-button" %>
<% end %>

