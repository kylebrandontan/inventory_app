<h1>stock</h1>
<!-- <hr /> -->
<table class="table">
  <tbody>
    <tr>
      <td>Warehouse</td>
      <td id="stock--<%= @stock.id %>_warehouse">
        <%= @stock.warehouse.full_address %>
      </td>
    </tr>
    <tr>
      <td>Product Name</td>
      <td id="stock--<%= @stock.id %>_product">
        <%= @stock.product.name %>
      </td>
    </tr>
    <tr>
      <td>Count</td>
      <td id="stock--<%= @stock.id %>_count">
        <%= @stock.count %>
      </td>
    </tr>
  </tbody>
</table>
